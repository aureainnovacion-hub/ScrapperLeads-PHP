# Configuración de Desarrollo
APP_NAME=ScrapperLeads
APP_ENV=development
APP_DEBUG=true
APP_URL=http://localhost:8000
APP_TIMEZONE=Europe/Madrid

# Base de Datos (SQLite para desarrollo)
DB_HOST=localhost
DB_PORT=3306
DB_NAME=scrapper_leads_dev
DB_USER=root
DB_PASSWORD=
DB_PREFIX=sl_
DB_CHARSET=utf8mb4

# Configuración del Scraper
SCRAPER_MAX_RESULTS=20
SCRAPER_TIMEOUT=30
SCRAPER_USER_AGENT="Mozilla/5.0 (compatible; ScrapperLeads-Dev/1.0)"
SCRAPER_DELAY=2

# Logging
LOG_LEVEL=debug
LOG_PATH=./logs

# FTP (no usado en desarrollo)
FTP_HOST=
FTP_USER=
FTP_PASSWORD=
FTP_PATH=/www/
FTP_PORT=21

